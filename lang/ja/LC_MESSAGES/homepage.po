msgid "Gold sponsors"
msgstr "ゴールドスポンサー"

#: sponsor.php:191
msgid "Early sponsors"
msgstr "初期のスポンサー"

#: sponsor.php:233
msgid "Silver sponsors"
msgstr "シルバースポンサー"

#: sponsor.php:258
msgid "Bronze sponsors"
msgstr "ブロンズスポンサー"

